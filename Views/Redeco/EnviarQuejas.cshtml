@using System.Globalization;

@model Condusef_DLL.Funciones.Condusef.Redeco
@{
    var cultura = new CultureInfo(@ViewData["Lang"].ToString() ?? "es");
    var rm = new System.Resources.ResourceManager(typeof(IdiomaCondusef_DLL.Condusef.Condusef));
    var rs = rm.GetResourceSet(cultura, true, true);
    ViewBag.Title = "Enviar quejas | " + rs.GetString("Redeco");
    ViewBag.TituloCard = rs.GetString("EnviarRedeco");
    Layout = "~/Views/Shared/_LayoutApp.cshtml";
}

<style>
    .right-padding-3 {
        padding: 0 5em 0 0;
    }

    .btn-info{
        color: white!important;
    }
</style>

<div class="container-fluid p-0">
    <div class="card table-borderless">
        @* <div class="card-header">
            <span class="card-title title">@rs.GetString("EnviarRedeco")</span>
        </div> *@
        <div class="card-body p-1">
            <div class="container-fluid p-5">
                <input type="hidden" id="institucion-clave" value="@User.FindFirst("Empresa")?.Value" />
                <input type="hidden" id="sector" value="@User.FindFirst("Sector")?.Value" />
                <input type="hidden" id="indice-fila" />
                <div class="flex-row row mb-4 justify-content-between">
                    <div class="col-2" >
                        <button class="btn btn-success btn-block" id="btn_abrirModalQueja" data-bs-toggle="modal" data-bs-target="#modalAgregarQueja"><i class="fas fa-plus" style="margin-right: 0.5rem"></i> @rs.GetString("AgregarQuejaBtn")</button>
                    </div>
                    <div class="col-2 ml-auto">
                        <button id="btn_agregarDatosFicticios" class="btn btn-warning btn-block"><i class="fa-solid fa-rotate" style="margin-right: 0.5rem"></i>@rs.GetString("AgregarDatosFicticios")</button>
                    </div>
                    <div class="col-2 ml-auto">
                        <button id="btn_subirQuejas" class="btn btn-info btn-block"><i class="fa-solid fa-cloud-arrow-up" style="margin-right: 0.5rem"></i>@rs.GetString("SubirQuejas")</button>
                    </div>
                </div>
                <div class="flex-row row mb-4 justify-content-between" style="display:none;">
                    <div class="col-2">
                        <button class="btn btn-success btn-block" id="btn_ListadoErrores" data-bs-toggle="modal" data-bs-target="#modalListadoErrores"><i class="fas fa-plus" style="margin-right: 0.5rem"></i> @rs.GetString("AgregarQuejaBtn")</button>
                        <button class="btn btn-success btn-block" id="btn_ErroresQueja" data-bs-toggle="modal" data-bs-target="#modalErroresQueja"><i class="fas fa-plus" style="margin-right: 0.5rem"></i> @rs.GetString("AgregarQuejaBtn")</button>
                    </div>
                </div>
                <div class="flex-row row mt-3">
                    <div class="col">
                        <div class="" id="Scroll">
                            <table class="table table-sm table-responsive w-100 d-block d-xl-table table-bordered table-striped selector dt-body-center pt-3" id="tablaQuejas" style="width:100%">
                                <thead class="dark-thead">
                                    <tr>
                                        <th></th>
                                        <th></th>
                                        <th class="">@rs.GetString("QuejaFolio")</th>
                                        <th class="">@rs.GetString("TQuejaFechaRecepcion")</th>
                                        <th class="">@rs.GetString("TMedioRecepcion")</th>
                                        <th class="">@rs.GetString("TNivelAtencion")</th>
                                        <th class="">@rs.GetString("ProductoServicio")</th>
                                        <th class="">@rs.GetString("QuejaCausa")</th>
                                        <th class="">@rs.GetString("QuejaEstatus")</th>
                                        <th class="">@rs.GetString("TipoPersona")</th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                        <th></th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            @* <button id="consulta-btn" class="btn btn-warning">Consulta</button> *@
        </div>
    </div>
</div>



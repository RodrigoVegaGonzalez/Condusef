                                </div>
                                <span class="input-group-text" id="anioActual"></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-group">
                            @Html.Label("", string.Format("{0}", rs.GetString("QuejaFolio")), new { @class = "label-control mb-2" })
                            <div class="col nopadding">
                                <div class="input-group">
                                    @Html.EditorFor(model => model.QuejasRedeco.QuejasFolio, new { htmlAttributes = new { @class = "form-control", @id = "QuejasFolio", @autocomplete = "off" } })

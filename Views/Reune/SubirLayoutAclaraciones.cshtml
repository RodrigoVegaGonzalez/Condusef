                                    <input class="form-check-input" type="radio" name="persona-radio" id="persona-radio1" value="1" checked>
                                    <label class="form-check-label" for="persona-radio1">
                                        @rs.GetString("Fisica")
                                    </label>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="persona-radio" id="persona-radio2" value="2">
                                    <label class="form-check-label" for="persona-radio">
                                        @rs.GetString("Moral")
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-3">
                        <div class="flex-row row">
                            <label class="mb-2" for="">@rs.GetString("Sexo")</label>
                            <div class="col-6">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="sexo-radio" id="sexo-radio1" value="H" checked>
                                    <label class="form-check-label" for="sexo-radio1">
                                        @rs.GetString("Hombre")
                                    </label>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="sexo-radio" id="sexo-radio2" value="M">
                                    <label class="form-check-label" for="sexo-radio2">
                                        @rs.GetString("Mujer")
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-2">
                        <div class="form-group">
                            @Html.Label("", string.Format("{0}", rs.GetString("Edad")), new { @class = "label-control mb-2" })
                            <div class="col nopadding">
                                <div class="input-group">
                                    @Html.EditorFor(model => model.AclaracionesGenerales.AclaracionEdad, new { htmlAttributes = new { @type = "number", @value = "18", @min = "18", @max = "100", @class = "form-control", @id = "AclaracionEdad", @autocomplete = "off" } })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="flex-row row my-3">
                    <div class="col-4">
                        <div class="flex-row row">
                            <label class="mb-2" for="">@rs.GetString("Monetario")</label>
                            <div class="col-6">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="monetario-radio" id="monetario-radio1" value="SI">
                                    <label class="form-check-label" for="monetario-radio1">
                                        @rs.GetString("Si")
                                    </label>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="monetario-radio" id="monetario-radio2" value="NO" checked>
                                    <label class="form-check-label" for="monetario-radio2">
                                        @rs.GetString("No")
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-3">
                        <div class="form-group">
                            @Html.Label("", string.Format("{0}", rs.GetString("MontoReclamado")), new { @class = "label-control mb-2" })
                            <div class="col nopadding">
                                <div class="input-group">
                                    @Html.EditorFor(model => model.AclaracionesGenerales.AclaracionMontoReclamado, new { htmlAttributes = new { @type = "number", @step = "0.01", @min = "0", @class = "form-control", @id = "AclaracionMontoReclamado", @autocomplete = "off" } })
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-5">
                        <div class="form-group">
                            @Html.Label("", string.Format("{0}", rs.GetString("FolioCondusef")), new { @class = "label-control mb-2" })
                            <div class="col nopadding">
                                <div class="input-group">
                                    @Html.EditorFor(model => model.AclaracionesGenerales.AclaracionFolioCondusef, new { htmlAttributes = new { @class = "form-control", @id = "AclaracionFolioCondusef", @autocomplete = "off" } })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="flex-row row my-3">
                    <div class="col-3">
                        <div class="flex-row row">
                            <label class="mb-2" for="">@rs.GetString("Reversa")</label>
                            <div class="col-6">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="reversa-radio" id="reversa-radio1" value="1">
                                    <label class="form-check-label" for="monetario-radio1">

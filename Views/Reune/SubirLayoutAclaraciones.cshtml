                                </div>
                            </div>
                            <div class="col-6">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="operacion-radio" id="operacion-radio2" value="NO" checked>
                                    <label class="form-check-label" for="operacion-radio2">
                                        @rs.GetString("No")

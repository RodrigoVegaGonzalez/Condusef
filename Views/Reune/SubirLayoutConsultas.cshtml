<div class="modal fade" id="modalSubirArchivo" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="exampleModalLabel">@rs.GetString("CargarArchivo")</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="flex-row row">
                    <div class="form-group col-12 p-3" id="Archivo_Amorti">
                        @Html.Label("", string.Format("{0}", rs.GetString("SeleccionarArchivo")), new { @class = "control-label mb-3" })
                        <div class="col nopadding input-group">
                            <label class="input-group-btn">
                                <span class="btn btn-primary">
                                    @rs.GetString("Buscar") &hellip;<input class="" type="file" style="display: none;" id="labelinput" name="UploadedFile" accept=".xlsx" />
                                </span>
                            </label>
                            <input class="file-path form-control" type="text" id="inputname" readonly>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                <button id="subirArchivo_btn" type="button" class="btn btn-primary">@rs.GetString("SubirArchivo")</button>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    <script type="text/javascript">

        var urlEnviarDatos = '@Url.Action("Enviar_Consultas", "Reune", new { Area = "" })';
        var urlSubirLayout = '@Url.Action("Subir_Layout_Consultas", "Reune", new { Area = "" })';

        var urlConsultarCausas = '@Url.Action("Reune_Consultar_Causas_Producto", "Reune", new { Area = "" })';
        var urlConsultarCP = '@Url.Action("Reune_Consultar_CP", "Reune", new { Area = "" })';
        var urlConsultarMunicipios = '@Url.Action("Reune_Consultar_Municipios", "Reune", new { Area = "" })';
